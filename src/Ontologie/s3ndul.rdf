     xmlns:old-ssn="&lt;http://purl.oclc.org/NET/ssnx/ssn#"
     xmlns:schema="http://schema.org/"
     xmlns:voaf="http://purl.org/vocommons/voaf#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:sosa="http://www.w3.org/ns/sosa/"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:s3n="http://w3id.org/s3n/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="&s3n;s3ndul">
        <dcterms:creator>SAGAR Samya -  Lab-STICC (imt-atlantique)</dcterms:creator>
        <dcterms:created>2018</dcterms:created>
        <vann:preferredNamespacePrefix>s3ndul</vann:preferredNamespacePrefix>
        <dcterms:title>S3N-DUL</dcterms:title>
        <vann:preferredNamespaceUri>http://w3id.org/s3n/s3ndul/</vann:preferredNamespaceUri>
        <dcterms:description>This ontology introduces the alignment of S3N to the DOLCE UltraLite upper ontology (DUL).</dcterms:description>
        <owl:imports rdf:resource="&s3n;S3NAlgorithm"/>
        <owl:imports rdf:resource="&s3n;S3NDatasheet"/>
        <owl:imports rdf:resource="http://www.loa.istc.cnr.it/ontologies/DUL.owl"/>
        <owl:imports rdf:resource="&ssn;dul"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/s3n/Algorithm -->

    <rdf:Description rdf:about="&s3n;Algorithm">
        <rdfs:subClassOf rdf:resource="&dul;Method"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/MaxValue -->

    <rdf:Description rdf:about="&s3n;MaxValue">
        <rdfs:subClassOf rdf:resource="&dul;Region"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/Measurand -->

    <rdf:Description rdf:about="&s3n;Measurand">
        <rdfs:subClassOf rdf:resource="&dul;Quality"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/MinValue -->

    <rdf:Description rdf:about="&s3n;MinValue">
        <rdfs:subClassOf rdf:resource="&dul;Region"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/Parameter -->

    <rdf:Description rdf:about="&s3n;Parameter">
        <rdfs:subClassOf rdf:resource="&dul;Quality"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/ParameterCapability -->

    <rdf:Description rdf:about="&s3n;ParameterCapability">
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/SensorReference -->

    <rdf:Description rdf:about="&s3n;SensorReference">
        <rdfs:subClassOf rdf:resource="&dul;InformationObject"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/SequenceNumber -->

    <rdf:Description rdf:about="&s3n;SequenceNumber">
        <rdfs:subClassOf rdf:resource="&dul;Region"/>
    </rdf:Description>
    


    <!-- http://w3id.org/s3n/Symbol -->

    <rdf:Description rdf:about="&s3n;Symbol">
        <rdfs:subClassOf rdf:resource="&dul;Region"/>
    </rdf:Description>
</rdf:RDF>



