    <!ENTITY voaf "http://purl.org/vocommons/voaf#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY dul "http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#" >
]>


<rdf:RDF xmlns="&sportdul;#"
     xml:base="http://w3id.org/sportdul"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:voaf="http://purl.org/vocommons/voaf#"
     xmlns:dul="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:sportdul="http://w3id.org/sportdul"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#">
    <owl:Ontology rdf:about="http://w3id.org/sportdul">
        <dcterms:description>This ontology introduces the alignment of SMS (Sport and MI modules) to the DOLCE UltraLite upper ontology (DUL).</dcterms:description>
        <dcterms:created>2018</dcterms:created>
        <vann:preferredNamespaceUri>http://w3id.org/sportdul/</vann:preferredNamespaceUri>
        <dcterms:creator>SAGAR Samya -  Lab-STICC (imt-atlantique)</dcterms:creator>
        <dcterms:title>Sport-DUL</dcterms:title>
        <vann:preferredNamespacePrefix>sportdul</vann:preferredNamespacePrefix>
        <owl:imports rdf:resource="http://w3id.org/MI"/>
        <owl:imports rdf:resource="http://w3id.org/Sport"/>
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/MI#PerformanceIndicator -->

    <rdf:Description rdf:about="http://w3id.org/MI#PerformanceIndicator">
        <rdfs:subClassOf rdf:resource="&dul;Quality"/>
    </rdf:Description>
    


    <!-- http://w3id.org/Sport#Duration -->

    <rdf:Description rdf:about="http://w3id.org/Sport#Duration">
        <rdfs:subClassOf rdf:resource="&dul;TimeInterval"/>
    </rdf:Description>
    


    <!-- http://w3id.org/Sport#Locale -->

    <rdf:Description rdf:about="http://w3id.org/Sport#Locale">
        <rdfs:subClassOf rdf:resource="&dul;Place"/>
    </rdf:Description>
    


    <!-- http://w3id.org/Sport#PhysicalActivity -->

    <rdf:Description rdf:about="http://w3id.org/Sport#PhysicalActivity">
        <rdfs:subClassOf rdf:resource="&dul;Task"/>
    </rdf:Description>
    


    <!-- http://w3id.org/Sport#Sport -->

    <rdf:Description rdf:about="http://w3id.org/Sport#Sport">
        <rdfs:subClassOf rdf:resource="&dul;Event"/>
    </rdf:Description>
    


    <!-- http://w3id.org/Sport#SportingPractice -->

    <rdf:Description rdf:about="http://w3id.org/Sport#SportingPractice">
        <rdfs:subClassOf rdf:resource="&dul;Action"/>
    </rdf:Description>
</rdf:RDF>



